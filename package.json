{
  "name": "firebug-ui",
  "version": "1.0.0",
  "description": "UI library for react and typescript",
  "license": "MIT",
  "keywords": [
    "react",
    "ts",
    "component library"
  ],
  "author": "bsmrdel101",
  "type": "module",
  "scripts": {
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build": "vite build",
    "prepublishOnly": "npm run build"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./styles": "./dist/index.css"
  },
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@storybook/addon-styling-webpack": "^3.0.0",
    "@storybook/react-vite": "^10.1.4",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "sass": "^1.94.2",
    "storybook": "^10.1.4",
    "typescript": "~5.9.3",
    "vite": "^7.2.6",
    "vite-plugin-dts": "^4.5.4"
  }
}
